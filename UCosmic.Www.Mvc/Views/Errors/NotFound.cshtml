@using UCosmic.Www.Mvc.Models
@{
    ViewBag.Title = "404 - Page not found";
}
@section bib
{
    <div class="bib" data-current-bib="errors">
        @Html.Partial(MVC.Errors.Views._Bib)
    </div>
}
<div class="fixed content" data-current-module="home">
    <header>
        <h1>@ViewBag.Title</h1>
    </header>
    <p>
        UCosmic was unable to find a web page or other resource at this URL.
    </p>
    <p>
        If you reached this page using a bookmark, please remove it and update it with the content's new location.
    </p>
    <p>
        If you reached this page by manually typing a URL into your browser, please don't.
    </p>
    <p>
        If you believe you have reached this page in error, please report your concerns using our 
        @Html.Partial(MVC.Shared.Views._UserVoiceLink, new UserVoiceLinkModel(Request, ViewData)) link.
    </p>
</div>
