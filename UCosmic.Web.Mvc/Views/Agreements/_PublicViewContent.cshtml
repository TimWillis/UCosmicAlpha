 <section id="publicView">
        <header>
            <h2>
                <em data-bind="text: name, visible: name"></em>
                <span data-bind="visible: name"><br/></span>
            </h2>
        </header>
        <div class="field-table w690-l190-v500" data-bind="visible: isBound" style="display:none;">
            <div>
                <span>
                    <span data-bind="html: participantsNames"></span>
                </span>
                <span style="font-size: 14px;">
                    <span data-bind="text: type()"></span>
                    , Status is:
                    <strong style="color: blue;" data-bind="text: status"></strong><br/>
                    This agreement is 
                    <span data-bind="visible: !isAutoRenew()">not </span>
                    auto-renewable.
                    <br/>
                    <strong data-bind="text: startsOnDate() + ' - '"></strong>
                    <strong data-bind="text:  expiresOnDate"></strong>
                </span>
            </div>
            <div style="padding-top: 20px;">
                <ul data-bind="foreach: files">
                    <li data-bind="element: 'fileE1'">
                        <div> 
                            <div class="on-left" style="padding-right: 10px;">
                                <div>
                                    <a title="View this file" target="_blank" data-bind="attr: { href: '/api/agreements/' + $root.agreementId.val + '/files/' + id() + '/content/' }"><strong data-bind="    text: customName, visible: !isEdit()"></strong></a>
                                </div>
                            </div>
                            <a title="Download this file" data-bind="attr: { href: '/api/agreements/' + $root.agreementId.val + '/files/' + id() + '/download/' }"><i class="download on-left"></i></a>
                               
                        </div>
                    </li>
                </ul>
            </div>
        </div>
    </section>