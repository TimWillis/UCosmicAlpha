@{
    ViewBag.Title = "Import Student Activities";
}

<div class="fixed content" data-current-module="students">
    <header>
        <h1>@ViewBag.Title</h1>
    </header>
   
    <form action="" method="post" enctype="multipart/form-data">

        <label for="file1">Filename:</label>
        <input type="file" name="file" id="file" />

        <input type="submit" />
    </form>
    <hr />

    <div>
        @if (ViewBag.Success != null)
        {
            <h2>@ViewBag.Success entries added successfully</h2>
            <h2>@ViewBag.Failure entries were duplicates or had errors</h2>
        }
    </div>

</div>
