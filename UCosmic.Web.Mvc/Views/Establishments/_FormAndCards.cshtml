<div data-side-swiper="root" id="search">
    <div data-side-swiper="deck">
        <div data-side-swiper="on" class="left">
            @Html.Partial(MVC.Establishments.Views._FormNamesSection)
            @Html.Partial(MVC.Establishments.Views._FormUrlsSection)
            <section id="location" class="fieldset">
                <header>
                    <h2>
                        Location
                    </h2>
                </header>
                <div>
                    <div style="display: none;">
                        Lat:
                        <input type="text" data-bind="value: toolsMarkerLat" />
                        Lng:
                        <input type="text" data-bind="value: toolsMarkerLng" />
                    </div>
                    <div id="map_canvas" class="map-canvas-w100-h500"></div>
                    @Html.Partial(MVC.Shared.Views._GoogleMapsToolsOverlay)
                </div>
            </section>
            <div data-bind="jqElement: '$genericAlertDialog'" title="" style="display: none;">
                <div>
                    <p class="content">
                    </p>
                </div>
            </div>
        </div>
    </div>
</div>
